#include <cmath>


int centuryFromYear(int year){ 


 if( year >= 1601 && year <=1700){
    return 17;
  }
  else if( year >= 1701 && year <= 1800){
    return 18;
  }
  else if(year >= 1801 && year <= 1900){
    return 19;
  }
  else if(year >= 1901 && year <= 2000){
    return 20;
  }
  return ceil(year / 100 + 1) ;
}
